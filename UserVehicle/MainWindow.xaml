<Window x:Class="UserVehicle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserVehicle"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        
        <Button x:Name="userBtn" Uid="0" Content="Add User" HorizontalAlignment="Left" Margin="150,34,0,0" VerticalAlignment="Top" Width="153" Height="44" Click="Button_Click"/>
        <Button x:Name="carBtn" Uid="1" Content="Add Car" HorizontalAlignment="Left" Margin="487,34,0,0" VerticalAlignment="Top" Width="153" Height="44" Click="Button_Click"/>


        <Frame x:Name="Main" HorizontalAlignment="Left" Height="320" Margin="10,90,0,0" VerticalAlignment="Top" Width="774" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
